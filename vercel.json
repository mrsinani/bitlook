{
  "rewrites": [
    { "source": "/api/lightning/:path*", "destination": "/api/lightning" },
    { "source": "/api/contracts/:path*", "destination": "/api/contracts" },
    { "source": "/api/auth/:path*", "destination": "/api/auth" },
    { "source": "/api/:path*", "destination": "/api/server" },
    { "source": "/proxy/:path*", "destination": "/api/lightning" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
