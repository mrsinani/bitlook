{
  "name": "bitlook-lightning-proxy",
  "version": "1.0.0",
  "description": "Proxy server for Lightning Network API calls",
  "main": "proxy.js",
  "type": "module",
  "scripts": {
    "prestart": "npm install",
    "start": "node proxy.js",
    "dev": "nodemon proxy.js",
    "debug": "node debug.js",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "all": "concurrently \"npm run start\" \"npm run server\""
  },
  "dependencies": {
    "@langchain/community": "^0.3.39",
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.63",
    "@langchain/openai": "^0.5.4",
    "@supabase/supabase-js": "^2.49.4",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "langchain": "^0.3.20"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.0.1"
  }
}
