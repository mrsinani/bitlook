  "scripts": {
    "dev": "vite",
    "server": "cross-env NODE_ENV=development node server/server.js",
    "server:prod": "cross-env NODE_ENV=production node server/server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lightning-proxy": "cd server && npm start",
    "debug:lightning": "cd server && npm run debug",
    "setup:lightning": "cd server && node setup-env.js",
    "hardhat:node": "hardhat node",
    "hardhat:deploy": "hardhat run scripts/deploy.cjs --network localhost",
    "start:contracts": "concurrently \"npm run hardhat:node\" \"sleep 5 && npm run hardhat:deploy\"",
    "start:all": "concurrently \"npm run start:contracts\" \"cd server && npm run all\" \"sleep 5 && npm run dev\"",
    "start:easy": "./start-bitlook.sh",
    "vercel-build": "tsc && vite build && cd server && npm install"
  },
